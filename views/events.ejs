    <main id="main">

      <section id="testimonials" class="blogs section-bg">
        
        <div class="container-fluid">
          <div class="section-title">
            <span>Upcoming Events</span>
            <h2>Upcoming Events</h2>
            <!-- <p>
             Our latest blogs.
            </p> -->
          </div>

          <!-- Blog data from json file -->
         
          <div class="row" id="card-area">
              <div class="card-group">
                <div
                  class="testimonials-slider swiper"
                  data-aos="fade-up"
                  data-aos-delay="100"
                >
              
                  <div class="swiper-wrapper">
                    <% eventData.latest_events.forEach(function(event) { %>
                      <div class="swiper-slide">
                        <div class="testimonial-item h-100 d-flex justify-content-center">
                          <div class="card h-100 ">
                            <img
                            src="<%= event.img %>"
                              class="card-img-top"
                              alt="..."
                            />
                            <div class="card-body d-flex flex-column ">
                              <h3 class="price-text"><%= event.title %></h3>

                              <table>
                                <tr>
                                  <td style="color:dimgray"> <i class="fa fa-calendar" style="padding: 1em;"></i><%= event.date %></td>
                                  <td style="color:dimgray"> <i class="fa fa-map-marker" style="padding: 1em;">  </i><%= event.location %></td>
                                </tr>
                              </table>

                              <p><%= event.description %></p>
                              <a href="<%=  %>"
                                class="w-100 mt-auto btn btn-lg btn-primary btn-card"
                              >
                              View More</a>
                            </div>
                          </div>
                        </div>
