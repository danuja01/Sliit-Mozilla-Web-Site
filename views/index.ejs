<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta content="width=device-width, initial-scale=1.0" name="viewport" />
  <meta name="keywords" content="SLIIT, SLIIT MOZILLA Community, SLIIT MOZILLA, Sri Lanka Institute of Information Technology, mozilla campus club in Sri Lanka" />
  <meta name="description" content="SLIIT MOZILLA Community. Firefox Student Ambassadors of Sri Lanka Institute of Information Technology" />
  <meta property="og:url" content="https://sliitmcc.herokuapp.com/" />
  <meta property="og:description" content="SLIIT MOZILLA Community. Firefox Student Ambassadors of Sri Lanka Institute of Information Technology" />
  <meta property="og:type" content="website" />
  <meta property="og:title" content="SLIIT MOZILLA Community" />


  <title>Mozilla Campus Club of SLIIT</title>


  <!-- Favicons -->
  <link href="assets/img/Mozilla_Firefox_icon-icons.com_66770.ico" rel="icon" />
  <link href="assets/img/Mozilla_Firefox_icon-icons.com_66770.ico" rel="apple-touch-icon" />

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet" />

  <!-- Bootstrap -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous" />

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js" integrity="sha384-b5kHyXgcpbZJO/tY9Ul7kGkf1S0CWuKcCD38l8YkeH8z8QjE0GmW1gYU5S9FOnJ0" crossorigin="anonymous"></script>

  <!-- Vendor CSS Files -->

  <link href="assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet" />
  <link href="assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet" />
  <link href="assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet" />
  <link href="assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet" />

  <!-- Template Main CSS File -->
  <link href="assets/css/style.css" rel="stylesheet" />
</head>

<body>
  <!-- ======= Header ======= -->
  <header id="header" class="fixed-top">
    <div class="container-fluid  ">

      <nav id="navbar" class="navbar navbar-expand-lg  bg-Secondary">
        <a href="/" class="logo navbar-brand"><img src="assets/img/logo.png" alt="" class="img-fluid" /> Mozilla
          Campus Club - SLIIT</a>
        <ul class="ms-auto">
          <li><a class="nav-link scrollto active" href="#header">Home</a></li>
          <li><a class="nav-link scrollto" href="#about">About</a></li>
          <li><a class="nav-link scrollto" href="#testimonials">Blogs</a></li>
          <li>
            <a class="nav-link scrollto" href="#event">Events</a>
          </li>
          <li><a class="nav-link scrollto" href="#">Contact Us</a></li>

        </ul>
        </li>
        </ul>


        <i class="bi bi-list mobile-nav-toggle"></i>
      </nav>
      <!-- .navbar -->
    </div>
  </header>
  <!-- End Header -->


  <section id="hero" class="d-flex align-items-center">
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-6 pt-5 pt-lg-0 order-2 order-lg-1 d-flex flex-column justify-content-center">
          <h1>Mozilla Campus Club Of SLIIT</h1>
          <h2>
            Firefox Student Ambassadors of Sri Lanka Institute of Information
            Technology
          </h2>
          <div class="d-flex">
            <a href="/contact_us" class="btn btn-primary btn-lg scrollto">CONTACT US</a>
            <a class="btn btn-lg btn-new-member"><i class="bi bi-person-plus-fill"></i> New Member</a>
          </div>
        </div>
        <div class="col-lg-6 order-1 order-lg-2 mx-auto hero-img">
          <img src="assets/img/hero-img.png" class=" img-fluid float-end hero-img hero-desktop" alt="" />
          <img src="assets/img/hero-mobile-img.png" class=" img-fluid mx-auto hero-img hero-mobile" alt="" />
        </div>
      </div>
    </div>
  </section>
  <!-- End Hero -->

  <main id="main">


    <!-- ======= About Section ======= -->
    <section id="about" class="about">
      <div class="container-fluid">
        <div class="row align-items-center">
          <div class="col-lg-5">
            <img src="assets/img/clubs-home.png" class="img-fluid" alt="" />
          </div>
          <div class="col-lg-7 pt-4 pt-lg-0 content ">
            <h3>
              What are we?
            </h3>
            <p>
              A Mozilla Club brings people together locally to build the open
              web on University and College campuses worldwide. The Internet is
              more than computers and code. The Internet is about people. And
              this is what Mozilla Clubs are all about: interacting with others
              to share, learn, teach, protect and build upon platforms, to make
              things that matter. Our Clubs are human structures that make the
              web real.
            </p>
          </div>
        </div>
      </div>
    </section>
    <!-- End About Section -->

    <!-- ======= Counts Section ======= -->
    <section id="counts" class="counts">
      <div class="container-fluid">
        <div class="row counters">
          <div class="col-lg-4 col-6 text-center">
            <span data-purecounter-start="0" data-purecounter-end="232" data-purecounter-duration="1" class="purecounter"></span>
            <p>Members</p>
          </div>

          <div class="col-lg-4 col-6 text-center">
            <span data-purecounter-start="0" data-purecounter-end="521" data-purecounter-duration="1" class="purecounter"></span>
            <p>Events</p>
          </div>

          <div class="col-lg-4 text-center">
            <span data-purecounter-start="0" data-purecounter-end="<%= blog.length %>" data-purecounter-duration="1" class="purecounter"></span>
            <p>Blogs</p>
          </div>


        </div>
      </div>
    </section>
    <!-- End Counts Section -->


    <!-- ======= Testimonials Section ======= -->
    <section id="testimonials" class="blogs section-bg">
      <div class="container-fluid">
        <div class="section-title">
          <span>BLOGS</span>
          <h2>BLOGS</h2>
          <p>
            Here you can discover various and updated information which can enhance
            your knowledge. We provide significant articles related to technology.
          </p>
        </div>
        <div class="card-group">
          <div class="testimonials-slider swiper" data-aos="fade-up" data-aos-delay="100">
            <div class="swiper-wrapper">
              <% blog.forEach(function(blog) { %>
              <div class="swiper-slide">
                <div class="testimonial-item h-100 d-flex justify-content-center">
                  <div class="card h-100 ">
                    <img src="<%= blog.image %>" class="card-img-top" alt="..." />
                    <div class="card-body d-flex flex-column">
                      <h3 class="price-text"><%= blog.title %></h3>
                      <p><%= blog.content.substring(0,190) + "..." %></p>
                      <a href="<%= blog.url %>" class="w-100 mt-auto btn btn-lg btn-primary btn-card">
                        Read More</a>
                    </div>
                  </div>
                </div>
              </div>
              <% }) %>
            </div>
          </div>
          <div class="swiper-pagination"></div>
        </div>
        <div class="blog-btn">
          <a href="/blogs" class="btn btn-outline-primary btn-lg">View More</a>
        </div>
      </div>

    </section>
    <!-- End Testimonials Section -->

    <!-- ======= Cta Section ======= -->
    <section id="cta" class="cta">
      <div class="container-fluid">
        <div class="text-center">
          <h3>Subscribe to our Blog Posts!</h3>
          <p>
            Subscribe to our blog posts via e-mail to get latest updates of our tech blogs!
          </p>
          <a class="btn cta-btn scrollto" href="#footer">SUBSCRIBE</a>
        </div>
      </div>
    </section>
    <!-- End Cta Section -->

    <!-- ======= Youtube Section ======= -->
    <section id="youtube">
      <div class="container-fluid">
        <div class="row">
          <div class="col-lg-6 pt-5 pt-lg-0 order-2 order-lg-1 d-flex flex-column justify-content-center">
            <h1>Catch us on
              YOUTUBE!</h1>
            <p>Here you can discover various and updated information which can enhance your knowledge. We provide significant Videos related to technology.</p>
            <div class="d-block">
              <a href="https://www.youtube.com/channel/UCx0d0jws6qZvjwiD8H8BlRg" class="btn btn-outline-danger btn-lg">Explore! <i class="bi bi-youtube"></i></a>
            </div>
          </div>
          <div class="col-lg-6 order-1 order-lg-2 mx-auto">
            <img src="./assets/img/youtube.png" class="img-fluid " alt="">
          </div>
        </div>
      </div>


    </section>




    <!-- ======= Event Section ======= -->
    <section id="events-card" class="event section-bg">
      <div class="container-fluid">
        <div class="section-title">
          <span>Check out our Upcoming Events!</span>
          <h2>Check out our Upcoming Events!</h2>

        </div>
        <div class="container">
          <div class="row">
            <% eventsData.forEach(event => { %>
            <div class="col-lg-4 col-md-4 col-sm-6">
              <div class="card h-100 card-margin">
                <img src="<%=event.img%>" class="events-card-img" alt="...">
                <div class="card-header no-border">
                  <h5 class="card-title"><%=event.title%></h5>
                </div>
                <div class="card-body" style="padding: 0 1rem;">
                  <div class="widget-49">
                    <div class="widget-49-title-wrapper">
                      <div class="widget-49-date-primary">
                        <span class="widget-49-date-day"><%=event.date%></span>
                        <span class="widget-49-date-month"><%=event.month%></span>
                      </div>
                      <div class="widget-49-meeting-info">
                        <span class="widget-49-pro-title"><%=event.location%></span>
                        <span class="widget-49-meeting-time"><%=event.time%></span>
                      </div>
                    </div>

                    <p class="widget-49-meeting-item"><%=event.description%></p>

                    <div class="widget-49-meeting-action">
                      <a href="#" class="mt-auto btn btn-sm btn-flash-border-primary">Register</a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <% }); %>





          </div>
        </div>
        <div class="blog-btn">
          <a href="/events" class="btn btn-outline-primary btn-lg">View More</a>
        </div>
      </div>
    </section>
    <!-- End Team Section -->

    <section id="team">
      <div class="container-fluid">
        <h1 class="team-title">Members Who Work Together!</h1>
        <div class="team-slider">

          <div id="testimonial-carousel" class="carousel slide" data-bs-ride="carousel" data-bs-interval="3000">
            <div class="carousel-inner">
              <div class="carousel-item active container-fluid">
                <img class="testimonial-img d-block mx-auto" src="./assets/img/team/Geethmaka Ayya.jpg" alt="dog-profile">
                <em>Geethmaka Dissanayake</em>
                <h4>PRESIDENT</h4>
              </div>
              <div class="carousel-item container-fluid">
                <img class="testimonial-img d-block mx-auto" src="./assets/img/team/Geethmaka Ayya.jpg" alt="lady-profile">
                <em>Pasindy Bandara</em>
                <h4>VICE PRESIDENT</h4>
              </div>
              <div class="carousel-item container-fluid">
                <img class="testimonial-img d-block mx-auto" src="./assets/img/team/Geethmaka Ayya.jpg" alt="lady-profile">
                <em>Chamathka Ariyarathne</em>
                <h4>SECRETARY</h4>
              </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#testimonial-carousel" data-bs-slide="prev">
              <span class="carousel-control-prev-icon"></span>
              <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#testimonial-carousel" data-bs-slide="next">
              <span class="carousel-control-next-icon"></span>
              <span class="visually-hidden">Next</span>
            </button>
          </div>
        </div>
      </div>
    </section>
  </main>
  <!-- End #main -->

  <!-- ======= Footer ======= -->
  <footer id="footer">
    <div class="footer-top">
      <div class="container-fluid">
        <div class="row justify-content-center">
          <div class="col-lg-6">
            <h3>Subscribe</h3>
            <p>
              Subscribe to stay in toch with our latest bllogs, events and more!
            </p>
          </div>
        </div>

        <div class="row footer-newsletter justify-content-center">
          <div class="col-lg-6">
            <form action="/" method="post">
              <input type="email" name="email" placeholder="Enter your Email"><input type="submit" name="submit" value="Subscribe">
            </form>
            <!-- <div class="alert-box ">
              Succesfully Subscribed!  <span class="float-end alert-closing" style="padding-right: 15px; ">X</span>
              </div> -->
          </div>
        </div>

        <div class="social-links">
          <a href="#" class="facebook"><i class="bx bxl-facebook"></i></a>
          <a href="#" class="instagram"><i class="bx bxl-instagram"></i></a>
          <a href="#" class="linkedin"><i class="bx bxl-linkedin"></i></a>
        </div>
      </div>
    </div>
  </footer>
  <!-- End Footer -->

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
  <script src="assets/vendor/purecounter/purecounter.js"></script>
  <script src="assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="assets/vendor/glightbox/js/glightbox.min.js"></script>
  <script src="assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
  <script src="assets/vendor/swiper/swiper-bundle.min.js"></script>

  <!-- Template Main JS File -->
  <script src="assets/js/main.js"></script>
</body>

</html>
